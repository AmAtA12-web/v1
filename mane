        local success, error = pcall(function()
            local args = { [1] = "ClaimPlaytime", [2] = i }
            bubbleRemote:WaitForChild("Function"):InvokeServer(unpack(args))
        end)
        if not success then
            warn("Failed to collect playtime " .. i .. ": " .. error)
        end
        task.wait(0.1)
    end
end

